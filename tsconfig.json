{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "src",
    "downlevelIteration": true,
    "paths": {
      "@app/*": [
        "app/*"
      ],
      "@menu/*": [
        "app/components/other pages/menu/*"
      ],
      "@home/*": [
        "app/components/home/*"
      ],
      "@work/*": [
        "app/components/work/*"
      ],
      "@contact/*": [
        "app/components/contact/*"
      ],
      "@agency/*": [
        "app/components/agency/*"
      ],
      "@case/*": [
        "app/components/cases/case/*"
      ],
      "@dobriy/*": [
        "app/components/cases/dobriy/*"
      ],
      "@acba/*": [
        "app/components/cases/acba/*"
      ],
      "@qit/*": [
        "app/components/cases/qit/*"
      ],
      "@beeline/*": [
        "app/components/cases/beeline/*"
      ],
      "@gyumri/*": [
        "app/components/cases/gyumri/*"
      ],
      "@dobriy-tv/*": [
        "app/components/cases/dobriy-tv/*"
      ],
      "@coca-cola/*": [
        "app/components/cases/coca-cola/*"
      ],
      "@order/*": [
        "app/components/order&submit/order/*"
      ],
      "@submit/*": [
        "app/components/order&submit/submit/*"
      ],
      "@assets/*": [
        "assets/*"
      ]
    },
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "types": ["gapi", "gapi.auth2"],
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ]
  }
}
